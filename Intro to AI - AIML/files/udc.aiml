<?xml version="1.0" encoding="UTF-8"?>
<aiml>
    
    <!-- Catch all category -->
    
    <category>
        <pattern>*</pattern>
        <template>I don't understand. Please retype your response.</template>
    </category>
    
    <!-- GREATER THAN OR EQUAL TO-->
    <category>
        <pattern>XGE <set>number</set> XS <set>number</set></pattern>
        <template>
            <think><set var="matched"><srai>XEQ <star/> XS <star index="2"/></srai></set></think>
            <condition var="matched">
                <li value="TRUE">TRUE</li>
                <li><srai>XGT <star/> XS <star index="2"/></srai></li>
            </condition>
        </template>
    </category>
    
    <!-- STRING EQUALS-->
    <category>
        <pattern>XEQ * XS *</pattern>
        <template>
            <learn>
                <category>
                    <pattern>
                        <eval><uppercase>XFALSE <star/></uppercase></eval>
                    </pattern>
                    <template>TRUE</template>
                </category>
            </learn>
            <srai>XFALSE <star index="2"/></srai>
            <learn>
                <category>
                    <pattern>
                        <eval><uppercase>XFALSE <star/></uppercase></eval>
                    </pattern>
                    <template>FALSE</template>
                </category>
            </learn>
        </template>
    </category>
    
    <!-- FALSE-->
    <category>
        <pattern>XFALSE *</pattern>
        <template>FALSE</template>
    </category>
    
    <!-- MAXIMUM NUMBER -->
    <category>
        <pattern>XMAX *</pattern>
        <template><star index="1"/></template>
    </category>    
    
    <category>
        <pattern>XMAX * *</pattern>
        <template>
            <srai>XXMAX <star index="2"/> XS <star index="1" /></srai>
        </template>
    </category>
    
    <category>
        <pattern>XXMAX *  XS *</pattern>
        <template>
            <think><set var="matched"><srai>XGE <star index="1" /> XS <star index="2" /></srai></set></think>
            <condition var="matched">
                <li value="TRUE"><star index="1" /></li>
                <li><star index="2"/></li>
            </condition>
        </template>
    </category>
    
    <category>
        <pattern>XXMAX * * XS *</pattern>
        <template>
            <think><set var="matched"><srai>XGE <star/> XS <star index="3"/></srai></set></think>
            <condition var="matched">
                <li value="TRUE"><srai>XXMAX <star index="2"/> XS <star/></srai></li>
                <li><srai>XXMAX <star index="2"/> XS <star index="3"/></srai></li>
            </condition>
        </template>
    </category>
    
